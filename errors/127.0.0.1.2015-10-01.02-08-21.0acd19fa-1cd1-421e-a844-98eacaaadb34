S'import random\nimport expression_evaluator\n\nclass Problem(object):\n\n    def __init__(self,givens,sol_alg):\n        self.sol_alg=sol_alg\n        self.givens=givens\n\n    def get_solution(self):\n        #replace algorithm with givens\n        for i in range(len(self.givens)):\n            v = "v"  + str(i)\n            given = str(self.givens[i])\n            self.sol_alg=self.sol_alg.replace(v,given)\n\n        #insert parsing algorithm here\n        parser=expression_evaluator.Parser()\n        solution=parser.parse(self.sol_alg).evaluate({})\n        return solution\n\n\ndef user():\n    return dict(form=auth())\n\n@auth.requires_login()\ndef index():\n    return locals()\n\ndef user_worksheets():\n    student_record=db(db.student.email==auth.user.email).select().last()\n    my_courses=db(db.student_course.student_id==student_record.id).select()\n    all_problem_types=db(db.problem_type).select()\n    #ditionary to associate problem types with classes\n    d={}\n    for r in all_problem_types:\n        if not d.has_key(r.course_id):\n            d[r.course_id]=[]\n        d[r.course_id].append(r)\n    return locals()\n\ndef generator():\n\n    session.jump_back=request.env.request_uri\n    problem_type=db.problem_type(request.args(0))\n    last_problem=db((db.problem.user_id==auth.user.id)&\\\n                    (db.problem.problem_type_id==problem_type)).select().last()\n    \n    #generate a new problem if no previous problems of this type exist\n    if last_problem is None:\n        #generate givens and add to list\n        \n        givens=[]\n        for i in range(problem_type.num_givens):\n            given=random.randint(1,20)\n            givens.append(given)\n\n        #generate problem prompt\n        prompt=problem_type.prompt%tuple(givens)\n\n        #create instance of Problem and its solution\n        problem=Problem(givens, problem_type.sol_alg)\n        solution=problem.get_solution()\n\n        #add problem and problem_givens to database\n        l=[]\n        problem_id=db.problem.insert(user_id=auth.user.id,problem_type_id=problem_type,answered=False,solution=solution)\n        for given in givens:\n            r=db.problem_given.insert(problem_id=problem_id,user_id=auth.user.id,problem_type_id=problem_type,given=given)\n            l.append(r)\n        #add score record to database for new problem\n        db.user_score.insert(user_id=auth.user.id,problem_type_id=problem_type.id,num_tries=0,num_solved=0)\n    \n    #pull last problem from the databse if the problem is not answered\n    elif last_problem.answered==False:\n        #pull last problem data from database\n        #generate list of problem_given records\n        given_records=db(db.problem_given.problem_id==last_problem).select()\n        #pull problem givens\n        givens = [r.given for r in given_records]\n        \n        prompt=problem_type.prompt%tuple(givens)\n        \n        problem=Problem(givens, problem_type.sol_alg)\n        solution=problem.get_solution()\n        \n    \n    #regenerate a new problem if the last problem is answered\n    elif last_problem.answered==True:\n        #generate givens and add to list\n        givens=[]\n        for i in range(problem_type.num_givens):\n            #if problem_type.name==\'addition\':\n            given=random.randint(1,20)\n            givens.append(given)\n\n        #create instance of Problem and its solution\n        problem=Problem(givens, problem_type.sol_alg)\n        solution=problem.get_solution()\n\n        #update problem.solution \n        last_problem.update_record(solution=solution)\n\n        #update problem_given records\n        #go through records where the problem type of the problem id == problem_type and where the givens match up\n        l = db((db.problem_given.user_id==auth.user.id)&\\\n                    (db.problem_given.problem_type_id==problem_type)).select()\n        for i in range(len(givens)):\n            l[i].update_record(given=givens[i])\n        prompt=problem_type.prompt%tuple(givens)\n        \n        #update problem answered\n        last_problem.update_record(answered=False)\n        \n\n    row = db((db.user_score.user_id==auth.user.id)&\\\n             (db.user_score.problem_type_id==problem_type.id)).select().first()\n    form=SQLFORM.factory(\n        Field(\'your_answer\',\'decimal(2,10)\'))\n    form[\'_style\']=\'border:4px solid black\'\n    correct_answer_button=False\n    #last_problem.update_record(answered=True)\n    \n    if form.process(onsuccess="thanks!").accepted and (form.vars.your_answer>.97*float(last_problem.solution) and form.vars.your_answer<1.03*float(last_problem.solution)):\n        row.update_record(num_tries=row.num_tries+1)\n        row.update_record(num_solved=row.num_solved+1)\n        #response.flash=\'Correct\'\n        correct_answer_button=True\n        last_problem.update_record(answered=True)\n        #redirect(URL(\'generator\'))\n    #elif conditions not working?\n    elif form.process(message_onsuccess="Incorrect! Try again :(",next=URL(\'generator\')).accepted and not (form.vars.your_answer>.97*float(last_problem.solution) and form.vars.your_answer<1.03*float(last_problem.solution)): \n    #form.vars.your_answer!=problem.get_solution():\n       row.update_record(num_tries=row.num_tries+1)\n       session.flash=\'Incorrect\'\n       #redirect(URL(\'generator\'))\n    return locals()\n\ndef student_profile():\n    student_record=db(db.student.email==auth.user.email).select().last()\n    my_courses=db(db.student_course.student_id==student_record.id).select()\n    all_problem_types=db(db.problem_type).select()\n    #ditionary to associate problem types with classes\n    d={}\n    for r in all_problem_types:\n        if not d.has_key(r.course_id):\n            d[r.course_id]=[]\n        d[r.course_id].append(r)\n    score_records=db(db.user_score.user_id==auth.user.id).select()\n    problem_type_scores={}\n    for r in score_records:\n       problem_type_scores[r.problem_type_id]=r.num_solved\n    return locals()\n\ndef create_problem():\n    form=SQLFORM(db.problem_type)\n    \n    if form.process().accepted:\n        session.flash=\'problem added!\'\n        redirect(URL(\'create_problem\'))\n    return locals()\n\ndef teacher_profile():\n    return locals()\n\ndef all_worksheets():\n    return locals()\n\nresponse._vars=response._caller(user)\n'
